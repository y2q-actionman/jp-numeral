(defun test-jp-old-fallback ()
  ;; too big num
  (assert-equal "壹無量大数" (jp-str :old (expt 10 68)))
  (assert-equal "一〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇"
		(jp-str :old (expt 10 72)))
  (assert-equal "負之壹無量大数" (jp-str :old (- (expt 10 68))))
  (assert-equal "−一〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇"
		(jp-str :old (- (expt 10 72))))
  ;; too small num
  (assert-equal "壹清浄" (jp-str :old (float (expt 10 -21))))
  (assert-equal "〇・〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇一"
		(jp-str :old (float (expt 10 -22))))
  (assert-equal "負之壹清浄" (jp-str :old (- (float (expt 10 -21)))))
  (assert-equal "−〇・〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇〇一"
		(jp-str :old (- (float (expt 10 -22)))))
  ;; complex
  (assert-equal (format nil "~A" #c(1 1))
		(jp-str :old #c(1 1)))
  ;;
  t)

(defun test-jp-old-parameters ()
  ;; [Integer]
  ;; digits-after-dot -- ignored
  (assert-equal "壹" (jp-str :old 1 :digits-after-dot nil))
  (assert-equal "壹" (jp-str :old 1 :digits-after-dot 0))
  (assert-equal "壹" (jp-str :old 1 :digits-after-dot 1))
  #+allegro(assert-equal "壹" (jp-str :old 1 :digits-after-dot -1))
  ;; scale
  (assert-equal "壹" (jp-str :old 1 :scale nil))
  (assert-equal "壹" (jp-str :old 1 :scale 0))
  (assert-equal "拾" (jp-str :old 1 :scale 1))
  (assert-equal "拾分之壹" (jp-str :old 1 :scale -1))
  ;; radix-point
  (assert-equal "壹" (jp-str :old 1 :radix-point nil))
  (assert-equal "壹a" (jp-str :old 1 :radix-point #\a))
  (assert-equal "壹abc" (jp-str :old 1 :radix-point "abc"))

  ;; [Rational]
  ;; digits-after-dot -- ignored
  (assert-equal "貳分之壹" (jp-str :old 1/2 :digits-after-dot nil))
  (assert-equal "貳分之壹" (jp-str :old 1/2 :digits-after-dot 0))
  (assert-equal "貳分之壹" (jp-str :old 1/2 :digits-after-dot 1))
  #+allegro(assert-equal "貳分之壹" (jp-str :old 1/2 :digits-after-dot -1))
  ;; scale
  (assert-equal "貳分之壹" (jp-str :old 1/2 :scale nil))
  (assert-equal "貳分之壹" (jp-str :old 1/2 :scale 0))
  (assert-equal "伍" (jp-str :old 1/2 :scale 1))
  (assert-equal "貳拾分之壹" (jp-str :old 1/2 :scale -1))
  ;; radix-point
  (assert-equal "貳分之壹" (jp-str :old 1/2 :radix-point nil))
  (assert-equal "貳分之壹a" (jp-str :old 1/2 :radix-point #\a))
  (assert-equal "貳分之壹abc" (jp-str :old 1/2 :radix-point "abc"))

  ;; [Float]
  ;; digits-after-dot
  (assert-equal "拾貳・參分肆釐" (jp-str :old 12.34 :digits-after-dot nil))
  (assert-equal "拾貳" (jp-str :old 12.34 :digits-after-dot 0))
  (assert-equal "拾貳・參分" (jp-str :old 12.34 :digits-after-dot 1))
  (assert-equal "拾貳・參分肆釐" (jp-str :old 12.34 :digits-after-dot 2))
  (assert-equal "拾貳・參分肆釐" (jp-str :old 12.34 :digits-after-dot 3))
  #+allegro(assert-equal "拾" (jp-str :old 12.34 :digits-after-dot -1))
  #+allegro(assert-equal "零" (jp-str :old 12.34 :digits-after-dot -2))
  ;; scale
  (assert-equal "拾貳・參分肆釐" (jp-str :old 12.34 :scale nil))
  (assert-equal "拾貳・參分肆釐" (jp-str :old 12.34 :scale 0))
  (assert-equal "壹佰貳拾參・肆分" (jp-str :old 12.34 :scale 1))
  (assert-equal "壹仟貳佰參拾肆" (jp-str :old 12.34 :scale 2))
  (assert-equal "壹・貳分參釐肆毫" (jp-str :old 12.34 :scale -1))
  (assert-equal "壹分貳釐參毫肆絲" (jp-str :old 12.34 :scale -2))
  ;; radix-point
  (assert-equal "拾貳・參分肆釐" (jp-str :old 12.34 :radix-point nil))
  (assert-equal "拾貳a參分肆釐" (jp-str :old 12.34 :radix-point #\a))
  (assert-equal "拾貳abc參分肆釐" (jp-str :old 12.34 :radix-point "abc"))
  (assert-equal "壹仟貳佰參拾肆abc" (jp-str :old 12.34 :radix-point "abc" :scale 2))
  (assert-equal "零abc壹分貳釐參毫肆絲" (jp-str :old 12.34 :radix-point "abc" :scale -2))
  ;; 
  t)

(defun test-jp-old ()
  (and (test-jp-old-integer-4digits)
       (test-jp-old-integer-bignums)
       (test-jp-old-integer-minus)
       (test-jp-old-rational)
       (test-jp-old-float)
       (test-jp-old-fallback)
       (test-jp-old-parameters)
       t))
